[core]
	  pager = LESSCHARSET=utf-8 less

[color]
    ui = true

[merge]
    ff = false

[push]
    default = simple

[alias]
    a = add
		b = branch
    ci = commit
    co = checkout
    cb = checkout -b
    d = diff
    dc = diff --cached --name-only
    dcn = diff --cached --name-only
    f = fech
    l = log
    lg = log --oneline --decorate --graph
    lga = log --oneline --decorate --graph --all
    rb = rebase
    rbc = rebase --continue
    rbi = rebase -i
		s = status
    sb = status -sb
    st = stash
    sp = stash pop
